{
    "name": "senet-eindhoven/azure-keyvault-php",
    "description": "Access Azure Keyvault from PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Senet\\AzureKeyVault\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Senet\\AzureKeyVault\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pim Jansen",
            "email": "pjansen@senet.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "guzzlehttp/psr7": "^2.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.1",
        "symfony/yaml": "^6.1",
        "bamarni/composer-bin-plugin": "^1.5",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.4"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "v0.1.0-dev"
        }
    }
}
